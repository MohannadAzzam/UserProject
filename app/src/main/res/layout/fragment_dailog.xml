<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context=".user.fragments.DailogFragment">


    <ImageView
        android:id="@+id/imageView_daulig"
        android:layout_width="@dimen/_60sdp"
        android:layout_height="@dimen/_60sdp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/_24sdp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/ic_true" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/_24sdp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView5__daulig"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/_20sdp"
            android:layout_marginRight="@dimen/_20sdp"
            android:fontFamily="@font/jf"
            android:text="تم تنفيذ طلبك"
            android:textColor="@color/black"
            android:textSize="@dimen/_24sdp" />

        <Button
            android:id="@+id/button__daulig"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_margin="@dimen/_24sdp"
            android:layout_marginTop="@dimen/_8sdp"
            android:fontFamily="@font/jf"
            android:text="تم "
            android:textSize="@dimen/_18sdp" />


    </LinearLayout>


</LinearLayout>